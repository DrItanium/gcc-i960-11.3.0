# Multilib configuration for i960
# Thanks copilot for providing this understanding

# Floating-point ABI
MULTILIB_OPTIONS = msoft-float
MULTILIB_DIRNAMES = soft-float

MULTILIB_EXCEPTIONS = 

MULTILIB_MATCHES = 

# Make sure that this makefile fragment is included
LIBGCC = stmp-multilib
INSTALL_LIBGCC = install-multilib

i960-c.o: $(srcdir)/config/i960/i960-c.c $(CONFIG_H) $(SYSTEM_H) coretypes.h $(TM_H) $(CPPLIB_H) $(TREE_H) c-family/c-pragma.h toplev.h $(GGC_H) $(TM_P_H)
	$(COMPILER) -c $(ALL_COMPILERFLAGS) $(ALL_CPPFLAGS) $(INCLUDES) $< 
